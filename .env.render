# Render Environment Variables Template

# PostgreSQL Database
# These will be automatically set by Render when using the render.yaml blueprint

# Python AI Service
GOOGLE_API_KEY=AIzaSyBws4xBo1bGCF3Tecqc68FBP44VhcnYY7E  # Replace with your API key
GEMINI_MODEL=gemini-1.5-flash
HOST=0.0.0.0
# PORT will be set by Render
PYTHONUNBUFFERED=1
PYTHONPATH=/workspace

# Java Backend
SERVER_PORT=8080  # Render will automatically map this to the external port
SPRING_DATASOURCE_URL=jdbc:postgresql://${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DATABASE}
SPRING_DATASOURCE_USERNAME=${POSTGRES_USER}
SPRING_DATASOURCE_PASSWORD=${POSTGRES_PASSWORD}
# JWT_SECRET will be auto-generated by Render
JWT_EXPIRATION=86400000
JAVA_OPTS=-XX:+UseZGC -XX:+UnlockExperimentalVMOptions -Xmx512m

# Frontend
NODE_VERSION=18.16.0
# VITE_API_URL will be set automatically by Render